{
    "manifest_version": 3,
    "name": "Neofoxify",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["foxify.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "neofox/neofox.png",
            "48": "neofox/neofox.png",
            "128": "neofox/neofox.png"
        }
    }    
}